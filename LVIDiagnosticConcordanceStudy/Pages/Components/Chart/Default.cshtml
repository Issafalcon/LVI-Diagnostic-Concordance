@using LVIDiagnosticConcordanceStudy.Models.ViewComponents

@model ChartValues

@{ 
    var myChart = new System.Web.Helpers.Chart(width: 600, height: 400, theme: System.Web.Helpers.ChartTheme.Green)
        .AddTitle("Cumulative Probability Curve")
        .AddSeries(
            name: "Theoretical",
            xValue: Model.TheoreticalSeriesYAxis,
            yValues: Model.TheoreticalSeriesYAxis)
        .AddSeries(
            name: "Observed",
            xValue: new int[] { Model.ObservedXValue},
            yValues: new decimal[] { Model.ObservedYValue })
        .Write();
}
